    "ShortId": 0, //暂不可用
    "GroupId": "", //分组编号，相同分组编号之间的客户端可见
    "GroupIds": [], //分组待选列表
    "Name": "", //客户端名，不填写的话，默认电脑名
    "Account": "", //账号，（暂不可用）
    "Password": "", //密码，（暂不可用）
    "Encode": false, //启用p2p加密传输
    "EncodePassword": "", //加密密钥32位，为空则每次加密随机密钥，如果填写，则各客户端都填写
